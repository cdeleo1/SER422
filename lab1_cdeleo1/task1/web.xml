<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>SER422 Lab 1: Task 1 Grocery List</display-name>
  <description>Lab 1: Task 1 SER422 Given App</description>
  <servlet>
    <servlet-name>GroceryList</servlet-name>
    <servlet-class>GroceryListServlet</servlet-class>
    <init-param>
    	<param-name>grocerylist</param-name>
  	</init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
	
  <servlet-mapping>
    <servlet-name>GroceryList</servlet-name>
    <url-pattern>/grocerylist</url-pattern>
  </servlet-mapping>

</web-app>
