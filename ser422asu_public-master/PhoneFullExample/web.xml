<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>SER422 Phone Full Example</display-name>
  <description>Lab 2 SER422 Given App</description>
  <servlet>
    <servlet-name>Phonebook</servlet-name>
    <servlet-class>edu.asupoly.ser422.PhoneServlet</servlet-class>
    <init-param>
    	<param-name>phonebook</param-name>
    	<param-value>phonebook.txt</param-value>
  	</init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
	
  <servlet-mapping>
    <servlet-name>Phonebook</servlet-name>
    <url-pattern>/phone</url-pattern>
  </servlet-mapping>

</web-app>
